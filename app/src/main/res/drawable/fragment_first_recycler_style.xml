<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <solid android:color="@color/fragment_first_recycler_selectedColor"/>
            <stroke android:color="@color/outInfoRecyclerBorder"
                    android:width="0.1dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <solid android:color="@color/fragment_first_recycler_unSelectedColor"/>
            <stroke android:color="@color/outInfoRecyclerBorder"
                    android:width="0.1dp"/>
        </shape>
    </item>
</selector>